{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-underscore-dangle": ["error",{"allow":["_id"]}]
    },
    "overrides": [
      {
        "files": ["*.js"],
        "rules": {
          "no-underscore-dangle": "off"
        }
      }
    ]
}
